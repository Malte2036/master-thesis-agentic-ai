{
  "name": "@master-thesis-agentic-rag/routing-agent",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/routing-agent/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/routing-agent",
        "main": "apps/routing-agent/src/index.ts",
        "tsConfig": "apps/routing-agent/tsconfig.lib.json",
        "assets": ["apps/routing-agent/*.md"]
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "@master-thesis-agentic-rag/routing-agent:build",
        "watch": true
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/routing-agent/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        },
        "no-cache": {
          "cache": false,
          "args": ["--clearCache"]
        }
      }
    }
  }
}
