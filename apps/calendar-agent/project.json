{
  "name": "@master-thesis-agentic-ai/calendar-agent",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/calendar-agent/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/calendar-agent",
        "main": "apps/calendar-agent/src/index.ts",
        "tsConfig": "apps/calendar-agent/tsconfig.json",
        "assets": [
          "apps/calendar-agent/src/assets",
          "apps/calendar-agent/package.json"
        ]
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "@master-thesis-agentic-ai/calendar-agent:build",
        "watch": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/apps/calendar-agent"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "{workspaceRoot}/coverage/apps/calendar-agent"
      }
    }
  },
  "tags": []
}
