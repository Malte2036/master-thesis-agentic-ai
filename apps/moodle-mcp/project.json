{
  "name": "@master-thesis-agentic-ai/moodle-mcp",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/moodle-mcp/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/moodle-mcp",
        "main": "apps/moodle-mcp/src/index.ts",
        "tsConfig": "apps/moodle-mcp/tsconfig.json",
        "assets": ["apps/moodle-mcp/src/assets", "apps/moodle-mcp/package.json"]
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "@master-thesis-agentic-ai/moodle-mcp:build",
        "watch": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/moodle-mcp/jest.config.ts"
      }
    }
  },
  "tags": []
}
