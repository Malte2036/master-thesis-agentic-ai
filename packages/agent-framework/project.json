{
  "name": "@master-thesis-agentic-ai/agent-framework",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/agent-framework/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/agent-framework",
        "main": "packages/agent-framework/src/index.ts",
        "tsConfig": "packages/agent-framework/tsconfig.lib.json",
        "assets": ["packages/agent-framework/*.md"]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/packages/agent-framework"],
      "options": {
        "cwd": "packages/agent-framework",
        "config": "packages/agent-framework/vitest.config.ts",
        "passWithNoTests": true,
        "reportsDirectory": "{workspaceRoot}/coverage/packages/agent-framework"
      }
    }
  }
}
