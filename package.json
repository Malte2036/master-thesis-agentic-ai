{
  "name": "master-thesis-agentic-rag",
  "private": true,
  "packageManager": "pnpm@9.7.0",
  "scripts": {
    "dev": "node scripts/dev-session.js \"pnpm run build:dist && concurrently 'nx run-many -t serve --projects=routing-agent,moodle-mcp,calendar-mcp,moodle-agent' 'pnpm run dev:chainlit'\"",
    "dev:routing-agent": "node scripts/dev-session.js \"nx serve routing-agent\"",
    "dev:moodle-agent": "node scripts/dev-session.js \"nx serve moodle-agent\"",
    "dev:moodle-mcp": "node scripts/dev-session.js \"nx serve moodle-mcp\"",
    "dev:calendar-mcp": "node scripts/dev-session.js \"nx serve calendar-mcp\"",
    "dev:chainlit": "node scripts/dev-session.js \"nx serve chainlit\"",
    "build": "nx run-many -t build",
    "build:dist": "nx build types && nx build agent-framework",
    "build:routing-agent": "nx build routing-agent",
    "build:moodle-mcp": "nx build moodle-mcp",
    "build:calendar-mcp": "nx build calendar-mcp",
    "build:agent-framework": "nx build agent-framework",
    "build:types": "nx build types",
    "lint": "nx run-many -t lint",
    "test": "nx run-many -t test",
    "test:routing-agent": "NX_SKIP_NX_CACHE=true nx test routing-agent",
    "clean": "nx run-many -t clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md,js,jsx,json}\"",
    "graph": "nx graph"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nx/eslint": "20.5.0",
    "@nx/eslint-plugin": "20.5.0",
    "@nx/jest": "20.5.0",
    "@nx/js": "20.5.0",
    "@nx/next": "^21.1.1",
    "@nx/node": "20.5.0",
    "@nx/workspace": "20.5.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "^29.5.12",
    "@types/node": "18.16.9",
    "concurrently": "^8.2.2",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "21.1.1",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.19.0"
  },
  "dependencies": {
    "zod": "^3.25.48"
  }
}
